from django.db import models

class {{ entity_name }}(models.Model):
    {% for prop, dtype in properties.items() %}
    {{ prop }} = models.{{ dtype }}()
    {% endfor %}

    def __str__(self):
        return self.{{ properties.keys()|first }}

    def to_dict(self):
        return {
            "id": self.id,
            {% for prop in properties.keys() %}
            "{{ prop }}": self.{{ prop }},
            {% endfor %}
        }