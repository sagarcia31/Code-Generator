{% set column_types = [] %}
{% for prop in properties %}
    {% if prop['type'] and prop['type'] not in column_types %}
        {% set _ = column_types.append(prop['type']) %}
    {% endif %}
{% endfor %}

{% if column_types %}
from sqlalchemy import Column, {{ ', '.join(column_types) }}
{% endif %}
from .db import db

class {{ entity_name }}(db.Model):
    {% for prop in properties %}
    {{ prop['name'] }} = Column({{ prop['type'] }}{% if prop.get('primary_key') %}, primary_key=True{% endif %})
    {% endfor %}